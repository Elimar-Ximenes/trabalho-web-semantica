@base <http://www.arida.ufc.br/mapeamentos/spotify/> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix mo: <http://purl.org/ontology/mo/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix schema: <http://schema.org/> .
@prefix svm: <http://www.arida.ufc.br/ontologies/music#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<#TrackMapping>
    rml:logicalSource [
        rml:source "musicas_novas_brasileiras_filtradas.csv" ;
        rml:referenceFormulation ql:CSV
    ] ;

    rr:subjectMap [
        rr:template "http://www.arida.ufc.br/resource/Spotify/Track/{track_id}" ;
        rr:class mo:Track ;
        rr:class svm:Track_Spotify ;
        rr:graph <https://graphdb.arida.site/repositories/EKG_MUSICA_BR/rdf-graphs/KG_Tracks_Spotify>
    ] ;

    rr:predicateObjectMap [
        rr:predicate dc:title ;
        rr:objectMap [ rml:reference "track_name_spotify" ; rr:language "pt" ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate foaf:homepage ;
        rr:objectMap [ rml:reference "track_url" ; rr:datatype xsd:anyURI ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate mo:track_number ;
        rr:objectMap [ rml:reference "track_number" ; rr:datatype xsd:integer ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate mo:duration ;
        rr:objectMap [ rml:reference "track_duration_ms" ; rr:datatype xsd:integer ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate schema:audio ;
        rr:objectMap [ rml:reference "track_preview_url" ; rr:datatype xsd:anyURI ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate svm:isExplicit ;
        rr:objectMap [ rml:reference "track_explicit" ; rr:datatype xsd:boolean ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate mo:ISRC ;
        rr:objectMap [ rml:reference "track_isrc" ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate mo:performer ;
        rr:objectMap [
            rr:template "http://www.arida.ufc.br/resource/Spotify/Artist/{artist_id}"
        ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate dc:identifier ;
        rr:objectMap [ rml:reference "track_id" ]
    ] .

